
public class Product {

	private String id;
	private String name;
	private String prdctCatog;
	private Double price;
	private Double discountAmount;
	private String description;
	private String trader;

	public Product(String id, String name, String prdctCategory, Double price, Double dicPri, String description,
			String trader) {
		this.id
		this.name
		this.prdctCatog
		this.description
		
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getTrader() {
		return trader;
	}

	public void setTrader(String trader) {
		this.trader = trader;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}

	public Double getDiscountAmount() {
		return discountAmount;
	}

	public void setDiscountAmount(Double discountAmount) {
		this.discountAmount = discountAmount;
	}

	public String getPrdctCatog() {
		return prdctCatog;
	}

	public void setPrdctCatog(String prdctCatog) {
		this.prdctCatog = prdctCatog;
	}

}
